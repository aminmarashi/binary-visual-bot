{
  "name": "binary-bot",
  "version": "2.3.2",
  "description": "Visual automation for binary.com",
  "main": "src/bot.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [],
    "ignore": [
      "./src/common/mock/*"
    ]
  },
  "scripts": {
    "start": "gulp watch",
    "test": "eslint src/ && jest",
    "test:full": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "gen-beta": "git checkout beta; npm prune ; npm install ; gulp build-min; rm -rf beta; mkdir beta ; cp -r www/ beta/beta; git checkout -- src",
    "gen-translation": "git checkout translation; npm prune ; npm install ; gulp build-min; rm -rf translation; mkdir translation ; cp -r www/ translation/translation; git checkout -- src",
    "deploy": "git checkout master ; npm prune ; npm install ; gulp deploy",
    "deploy-all": "npm run gen-beta; npm run gen-translation; npm run deploy"
  },
  "author": "Amin Marashi",
  "license": "MIT",
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "binary-charts": "^9.2.6",
    "binary-common-utils": "^4.9.0",
    "binary-indicators": "^1.5.2",
    "binary-live-api": "^28.1.0",
    "binary-mock-websocket": "^4.1.0",
    "blockly": "google/blockly.git#0785a7b",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "deep-diff": "^0.3.4",
    "del": "^2.2.2",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.9.0",
    "filesaverjs": "^0.2.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.13",
    "gulp-concat-css": "^2.3.0",
    "gulp-concat-util": "^0.5.5",
    "gulp-connect": "^5.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-mustache-plus": "^1.0.1",
    "gulp-open": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.3.1",
    "gulp-watch": "^4.3.10",
    "gulp-webpack": "^1.5.0",
    "i18next-scanner": "^1.5.0",
    "jest-cli": "^18.1.0",
    "jquery": "2.2.4",
    "json-loader": "^0.5.4",
    "loader-utils": "^0.2.16",
    "lz-string": "^1.4.4",
    "mocha": "^3.1.0",
